((arr: scala.Array[scala.Int], f: scala.Function1[scala.Int, scala.Unit]) => {
  val size: scala.Int = arr.length
  var i: scala.Int = 0
  while (i.<(size)) {
    val element: scala.Int = arr.apply(i)
    f.apply(element)
    i = i.+(1)
  }
})

