scala> { case class A(); def foo = 1; 2 }
// defined case class A
def foo: Int
val res0: Int = 2

scala> foo
val res1: Int = 1

scala> A()
val res2: A = A()

scala> { def f: Int = g; def g: Int = 5 }
def f: Int
def g: Int

scala> f + g
val res3: Int = 10

scala> { val x = 3; 4; val y = 5 }
val x: Int = 3
val res4: Int = 4
val y: Int = 5
