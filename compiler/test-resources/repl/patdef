scala> val Const, x = 0
val Const: Int = 0
val x: Int = 0
scala> val (Const, List(`x`, _, a), b) = (0, List(0, 1337, 1), 2)
val a: Int = 1
val b: Int = 2
scala> val a@b = 0
val a: Int = 0
val b: Int = 0
scala> val Y = 2
val Y: Int = 2
scala> val X, Y = 3
val X: Int = 3
val Y: Int = 3
scala> val foo = Y
val foo: Int = 3
scala> val X @ List(_) = List(1)
val X: List[Int] = List(1)
scala> val _ @ _ = 1
scala> val _ @ List(x) = List(1)
val x: Int = 1
scala> val List(_ @ List(x)) = List(List(2))
val x: Int = 2
